        <img src="https://media.licdn.com/dms/image/v2/C4E03AQG2WKzrYw-UKg/profile-displayphoto-shrink_800_800/profile-displayphoto-shrink_800_800/0/1664206781622?e=1752105600&v=beta&t=pQ2rLM_LmAE2sfdmACorkWX3pThvqpeq-wawy6uIFhA" alt="John Carter" class="profile-photo">
      </div>
      <form class="contact-form" id="contactForm">
        <div class="form-group">
          <input type="text" id="name" name="name" placeholder="Name" required>
        </div>
        <div class="form-group">
          <input type="email" id="email" name="email" placeholder="Email" required>
        </div>
        <div class="form-group">
          <textarea id="message" name="message" placeholder="Message" required></textarea>
        </div>
        <button type="submit" class="submit-button">Send Message</button>
      </form>
    </section>
  </main>

  <footer>
    <div class="social-icons">
      <a href="https://www.linkedin.com/in/john-carter-231b5a251/" target="_blank" rel="noopener">
        <svg width="24" height="24" fill="currentColor" viewBox="0 0 24 24">
          <path d="M19 0h-14c-2.761 0-5 2.239-5 5v14c0 2.761 2.239 5 5 5h14c2.762 0 5-2.239 5-5v-14c0-2.761-2.238-5-5-5zm-11 19h-3v-11h3v11zm-1.5-12.268c-.966 0-1.75-.79-1.75-1.764s.784-1.764 1.75-1.764 1.75.79 1.75 1.764-.783 1.764-1.75 1.764zm13.5 12.268h-3v-5.604c0-3.368-4-3.113-4 0v5.604h-3v-11h3v1.765c1.396-2.586 7-2.777 7 2.476v6.759z"/>
        </svg>
