    /* Hamburger menu for mobile */
    .mobile-nav-toggle {
      display: none;
      position: absolute;
      right: 24px;
      top: 24px;
      width: 40px;
      height: 40px;
      background: var(--primary);
      border-radius: 50%;
      align-items: center;
      justify-content: center;
      cursor: pointer;
      z-index: 20;
      box-shadow: 0 2px 8px rgba(110,160,233,0.10);
    }
    .mobile-nav-toggle span {
      display: block;
      width: 24px;
      height: 3px;
      background: var(--secondary);
      margin: 4px 0;
      border-radius: 2px;
      transition: all 0.3s;
