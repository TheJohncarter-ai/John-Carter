    .rocket-animated-wrapper {
      display: flex;
      align-items: center;
      justify-content: center;
      margin: 0 auto;
      width: 320px;
      height: 340px;
      position: relative;
      z-index: 1;
    }
    .animated-shuttle {
      animation: shuttleLaunch 3.5s cubic-bezier(.4,1.6,.6,1) infinite alternate;
    }
    .shuttle-group { /* Rocket group floats up */
      transform-origin: 160px 180px;
      animation: shuttleFloat 2.5s ease-in-out infinite alternate;
    }
    .plume {
      opacity: 0.85;
      filter: blur(0.5px);
      transform-origin: 160px 250px;
      animation: plumeFlicker 1.1s infinite alternate;
    }
    .smoke-clouds path {
      stroke: #6ea0e9;
      filter: blur(0.5px);
      animation: smokePulse 2.2s ease-in-out infinite alternate;
    }
    @keyframes shuttleLaunch {
      0% { transform: translateY(0); }
      100% { transform: translateY(-30px); }
    }
    @keyframes shuttleFloat {
      0% { transform: translateY(0); }
      100% { transform: translateY(-10px); }
    }
    @keyframes plumeFlicker {
      0% { opacity: 0.7; filter: blur(1.5px); }
      100% { opacity: 1; filter: blur(0.2px); }
    }
    @keyframes smokePulse {
      0% { opacity: 0.7; stroke-width: 3; }
      100% { opacity: 1; stroke-width: 5; }
    }
    /* Enhanced Hero Styles */
    .hero-name {
      font-family: 'Dancing Script', cursive;
      font-size: 5.5rem;
      color: var(--primary);
      margin-bottom: 32px;
      position: relative;
      z-index: 2;
      letter-spacing: 0.1em;
      text-shadow: 0 2px 16px rgba(0,0,0,0.3);
      animation: glow 2s ease-in-out infinite alternate;
    }

    .hero-desc {
      font-family: 'Cinzel', serif;
      font-size: 1.3rem;
      color: var(--text);
      margin-bottom: 0;
      letter-spacing: 0.25em;
      position: relative;
      z-index: 1;
      text-shadow: 0 2px 8px rgba(0,0,0,0.3);
    }

    .highlight-text {
      position: relative;
      display: inline-block;
      padding: 4px 12px;
      background: rgba(110,160,233,0.1);
      border-radius: 4px;
      transition: all 0.3s ease;
    }

    .highlight-text:hover {
      background: rgba(110,160,233,0.2);
      transform: translateY(-2px);
      box-shadow: 0 4px 12px rgba(110,160,233,0.15);
    }

    .highlight-text:active {
      transform: translateY(0);
      box-shadow: 0 2px 6px rgba(110,160,233,0.1);
    }

    @keyframes float {
      0% { transform: translateY(0) rotate(0deg); }
      50% { transform: translateY(-20px) rotate(5deg); }
      100% { transform: translateY(0) rotate(0deg); }
    }

    @keyframes glow {
      from {
        text-shadow: 0 0 10px rgba(110,160,233,0.5),
                     0 0 20px rgba(110,160,233,0.3),
                     0 0 30px rgba(110,160,233,0.2);
      }
      to {
        text-shadow: 0 0 20px rgba(110,160,233,0.6),
                     0 0 30px rgba(110,160,233,0.4),
                     0 0 40px rgba(110,160,233,0.3);
      }
    }

    .stat-item {
      position: relative;
      padding: 16px;
      background: rgba(110,160,233,0.05);
      border-radius: 12px;
      transition: all 0.3s ease;
    }

    .stat-item:hover {
      transform: translateY(-5px);
      background: rgba(110,160,233,0.1);
    }

    .stat-number {
      position: relative;
      display: inline-block;
    }

    .stat-number::after {
      content: '';
      position: absolute;
      bottom: -4px;
      left: 0;
      width: 100%;
      height: 2px;
      background: var(--primary);
      transform: scaleX(0);
      transition: transform 0.3s ease;
    }

    .stat-item:hover .stat-number::after {
      transform: scaleX(1);
    }

    @media (max-width: 768px) {
      .hero-stats {
        flex-direction: column;
        gap: 24px;
      }
      
      .hero-name {
        font-size: 3.5rem;
      }
      
      .hero-desc {
        font-size: 1.1rem;
      }
    }
    .odds-and-ends-link {
      color: #FF5C00;
      text-decoration: none;
      font-weight: bold;
      transition: all 0.3s ease;
      position: relative;
      display: inline-block;
    }
    .odds-and-ends-link:hover {
      color: #FF7F33;
      transform: translateY(-1px);
    }
    .odds-and-ends-link::after {
      content: '';
      position: absolute;
      bottom: -2px;
      left: 0;
      width: 100%;
      height: 2px;
      background: #FF5C00;
      transform: scaleX(0);
      transition: transform 0.3s ease;
    }
    .odds-and-ends-link:hover::after {
      transform: scaleX(1);
    }
    /* Add at the beginning of the style section */
    .scroll-progress {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 3px;
      background: linear-gradient(to right, var(--primary), var(--accent));
      transform-origin: 0 50%;
      transform: scaleX(0);
      z-index: 1000;
      transition: transform 0.1s ease-out;
    }
    /* Refined Geometric Pattern */
    .geometric-pattern {
      position: absolute;
      width: 100%;
      height: 100%;
      top: 0;
      left: 0;
      pointer-events: none;
      opacity: 0.08;
      z-index: 0;
      mix-blend-mode: overlay;
    }

    .geometric-pattern::before,
    .geometric-pattern::after {
      content: '';
      position: absolute;
      width: 400px;
      height: 400px;
      border: 1.5px solid var(--primary);
      opacity: 0.15;
      transition: all 0.8s cubic-bezier(0.4, 0, 0.2, 1);
      filter: blur(0.5px);
    }

    .geometric-pattern::before {
      top: -200px;
      right: -200px;
      transform: rotate(45deg);
      border-radius: 65% 35% 30% 70% / 60% 30% 70% 40%;
      animation: morphShape 12s ease-in-out infinite;
      box-shadow: 0 0 30px rgba(110, 160, 233, 0.1);
    }

    .geometric-pattern::after {
      bottom: -200px;
      left: -200px;
      transform: rotate(-45deg);
      border-radius: 35% 65% 70% 30% / 50% 60% 30% 60%;
      animation: morphShape 12s ease-in-out infinite reverse;
      box-shadow: 0 0 30px rgba(110, 160, 233, 0.1);
    }

    @keyframes morphShape {
      0% {
        border-radius: 65% 35% 30% 70% / 60% 30% 70% 40%;
        transform: rotate(45deg) scale(1);
      }
      50% {
        border-radius: 35% 65% 70% 30% / 50% 60% 30% 60%;
        transform: rotate(45deg) scale(1.05);
      }
      100% {
        border-radius: 65% 35% 30% 70% / 60% 30% 70% 40%;
        transform: rotate(45deg) scale(1);
      }
    }

    .section:hover .geometric-pattern::before,
    .section:hover .geometric-pattern::after {
      opacity: 0.2;
      filter: blur(0px);
    }

    .section:hover .geometric-pattern::before {
      transform: rotate(45deg) scale(1.1);
      box-shadow: 0 0 40px rgba(110, 160, 233, 0.15);
    }

    .section:hover .geometric-pattern::after {
      transform: rotate(-45deg) scale(1.1);
      box-shadow: 0 0 40px rgba(110, 160, 233, 0.15);
    }

    /* Add subtle gradient overlay */
    .section::after {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: radial-gradient(circle at center, transparent 0%, rgba(16, 18, 20, 0.1) 100%);
      pointer-events: none;
      z-index: 1;
    }

    /* Update section styles */
    .section {
      position: relative;
      overflow: hidden;
      transition: transform 0.3s ease-out;
    }

    .section:hover {
      transform: translateY(-2px);
    }

    /* Enhance content container */
    .section > div {
      position: relative;
      z-index: 2;
    }

    /* Geometric Icons */
    .geometric-icon {
      width: 80px;
      height: 80px;
      position: relative;
      display: flex;
      align-items: center;
      justify-content: center;
      transition: all 0.3s ease;
    }

    .geometric-icon::before,
    .geometric-icon::after {
      content: '';
      position: absolute;
      border: 2px solid var(--primary);
      transition: all 0.3s ease;
    }

    /* Investment Icon - Hexagon with Arrow */
    .icon-investment::before {
      width: 60px;
      height: 60px;
      transform: rotate(30deg);
      border-radius: 8px;
    }

    .icon-investment::after {
      width: 20px;
      height: 20px;
      border-left: none;
      border-bottom: none;
      transform: rotate(45deg) translate(15px, -15px);
    }

    /* Development Icon - Cube with Code */
    .icon-development::before {
      width: 50px;
      height: 50px;
      transform: rotate(45deg);
      border-radius: 4px;
    }

    .icon-development::after {
      width: 30px;
      height: 30px;
      border: none;
      border-right: 2px solid var(--primary);
      border-bottom: 2px solid var(--primary);
      transform: rotate(45deg) translate(10px, -10px);
    }

    /* Consulting Icon - Network Nodes */
    .icon-consulting::before {
      width: 40px;
      height: 40px;
      border-radius: 50%;
    }

    .icon-consulting::after {
      width: 60px;
      height: 60px;
      border: none;
      border-top: 2px solid var(--primary);
      border-right: 2px solid var(--primary);
      transform: rotate(45deg);
    }

    /* Hover Effects */
    .geometric-icon:hover::before {
      transform: rotate(45deg) scale(1.1);
      border-color: var(--accent);
    }

    .geometric-icon:hover::after {
      transform: rotate(45deg) scale(1.1);
      border-color: var(--accent);
    }

    .icon-investment:hover::before {
      transform: rotate(30deg) scale(1.1);
    }

    .icon-investment:hover::after {
      transform: rotate(45deg) translate(20px, -20px);
    }

    .icon-consulting:hover::before {
      transform: scale(1.1);
    }

    .icon-consulting:hover::after {
      transform: rotate(45deg) scale(1.1);
    }

    /* Remove Geometric Icons styles */
    .geometric-icon,
    .icon-investment,
    .icon-development,
    .icon-consulting {
      display: none;
    }

    /* Restore original service icons */
    .service-icon {
      display: flex;
      align-items: center;
      justify-content: center;
      width: 80px;
      height: 80px;
      margin: 0 20px;
      flex-shrink: 0;
    }

    /* Modern UI Enhancements */
    .glass-effect {
      background: rgba(16,18,20,0.8);
      backdrop-filter: blur(10px);
      border: 1px solid rgba(110,160,233,0.1);
      box-shadow: var(--shadow-lg);
    }

    .hover-lift {
      transition: var(--transition-medium);
    }

    .hover-lift:hover {
      transform: translateY(-4px);
      box-shadow: var(--shadow-lg);
    }

    .hover-glow {
      transition: var(--transition-medium);
    }

    .hover-glow:hover {
      box-shadow: 0 0 20px rgba(110,160,233,0.3);
    }

    .text-gradient {
      background: var(--gradient-primary);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
    }

    .border-gradient {
      border: 2px solid transparent;
      background: linear-gradient(var(--background), var(--background)) padding-box,
                var(--gradient-primary) border-box;
    }

    /* Enhanced Animations */
    @keyframes float {
      0% {
        transform: translateY(0) rotate(0deg);
      }
      50% {
        transform: translateY(-20px) rotate(5deg);
      }
      100% {
        transform: translateY(0) rotate(0deg);
      }
    }

    @keyframes pulse {
      0% {
        transform: scale(1);
        opacity: 1;
      }
      50% {
        transform: scale(1.05);
        opacity: 0.8;
      }
      100% {
        transform: scale(1);
        opacity: 1;
      }
    }

    @keyframes shimmer {
      0% {
        background-position: -200% center;
      }
      100% {
        background-position: 200% center;
      }
    }

    .animate-float {
      animation: float 6s ease-in-out infinite;
    }

    .animate-pulse {
      animation: pulse 2s ease-in-out infinite;
    }

    .animate-shimmer {
      background: linear-gradient(
        90deg,
        transparent 0%,
        rgba(110,160,233,0.1) 50%,
        transparent 100%
      );
      background-size: 200% 100%;
      animation: shimmer 3s linear infinite;
    }

    /* Scroll Animations */
    .fade-up {
      opacity: 0;
      transform: translateY(20px);
      transition: opacity 0.6s ease-out, transform 0.6s ease-out;
    }

    .fade-up.visible {
      opacity: 1;
      transform: translateY(0);
    }

    /* Loading States */
    .loading {
      position: relative;
      overflow: hidden;
    }

    .loading::after {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: linear-gradient(
        90deg,
        transparent 0%,
        rgba(110,160,233,0.1) 50%,
        transparent 100%
      );
      background-size: 200% 100%;
      animation: shimmer 1.5s linear infinite;
    }

    /* Interactive Elements */
    .interactive {
      cursor: pointer;
      transition: var(--transition-medium);
    }

    .interactive:hover {
      transform: scale(1.05);
      box-shadow: var(--shadow-md);
    }

    .interactive:active {
      transform: scale(0.98);
    }

    /* Accessibility */
    .visually-hidden {
      position: absolute;
      width: 1px;
      height: 1px;
      padding: 0;
      margin: -1px;
      overflow: hidden;
      clip: rect(0, 0, 0, 0);
      white-space: nowrap;
      border: 0;
    }

    /* Focus States */
    :focus {
      outline: 2px solid var(--primary);
      outline-offset: 2px;
    }

    :focus:not(:focus-visible) {
      outline: none;
    }

    :focus-visible {
      outline: 2px solid var(--primary);
      outline-offset: 2px;
    }
  </style>
</head>
